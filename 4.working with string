string1="hello"
string2="monk"
print(string1[:-1]) #1
print(string1[:4])  #2
print(string1[-1])  #3
print(string1 * 2)  #4
print(string1[:-1] + string2 + string1[:-1])  #5
print (string2[1])  #6
print (string2[3])  #7
print(string1 * 2 + string1[:-1] + string2) #8


# output
#1 hell
#2 hell
#3 o
#4 hellohello
#5 hellmonkhell
#6 o
#7 k
#8 hellohellohellmonk
